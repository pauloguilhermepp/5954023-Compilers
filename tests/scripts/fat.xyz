// fat.xyz
fn fatorial(n i64) {
        var
           i : i64 = 1,
           r : i64 = 1;
    
        if n < 0 {
            return -1;
        }

        while i < n + 1 {
            r = r * i;
            i++;
        }
        return r;
}
    
fn main() {
        var 
           i : i64 = 3,
           f : i64 = 0;
    
        f = fatorial(i);

        return 0;
}